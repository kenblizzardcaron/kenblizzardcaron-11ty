{
  "name": "11ty-resume-template",
  "version": "1.0.0",
  "description": "This template can be used to create new resumes with 11ty.",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all -p dev:*",
    "build": "run-s build:* build:pdf",
    "dev:11ty": "eleventy --serve",
    "dev:css": "tailwindcss -i src/assets/css/tailwind.css -o _site/assets/css/styles.css --watch --postcss",
    "build:11ty": "eleventy",
    "build:css": "tailwindcss -i src/assets/css/tailwind.css -o _site/assets/css/styles.css --postcss",
    "build:pdf": "node generate-pdf.js"
  },
  "keywords": [
    "template",
    "11ty",
    "eleventy",
    "resume",
    "cv",
    "css",
    "tailwindcss",
    "postcss",
    "html",
    "javascript",
    "nodejs",
    "npm",
    "github"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-img": "^3.1.8",
    "@netlify/plugin-lighthouse": "^6.0.3",
    "autoprefixer": "^10.4.16",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.32",
    "puppeteer": "^24.15.0",
    "tailwindcss": "^3.4.0"
  },
  "dependencies": {
    "markdown-it": "^14.1.0"
  }
}
